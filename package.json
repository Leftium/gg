{
	"name": "@leftium/gg",
	"version": "0.0.20",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Leftium/gg.git"
	},
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && node scripts/bundle-debug.js && svelte-package && node scripts/fix-debug-import.js && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"patches/"
	],
	"sideEffects": [
		"**/*.css"
	],
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.1",
		"@eslint/js": "^9.39.2",
		"@picocss/pico": "^2.1.1",
		"@rollup/plugin-commonjs": "^29.0.0",
		"@rollup/plugin-node-resolve": "^16.0.3",
		"@rollup/plugin-terser": "^0.4.4",
		"@sveltejs/adapter-vercel": "^6.3.0",
		"@sveltejs/kit": "^2.49.4",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@types/debug": "^4.1.12",
		"@types/node": "^25.0.8",
		"add": "^2.0.6",
		"debug": "^4.4.3",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.14.0",
		"globals": "^17.0.0",
		"prettier": "^3.7.4",
		"prettier-plugin-svelte": "^3.4.1",
		"publint": "^0.3.16",
		"supports-color": "^10.2.2",
		"svelte": "^5.46.3",
		"svelte-check": "^4.3.5",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.0",
		"vite": "^7.3.1",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"keywords": [
		"svelte"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		],
		"patchedDependencies": {
			"debug@4.4.3": "patches/debug@4.4.3.patch"
		}
	},
	"dependencies": {
		"dotenv": "^17.2.3",
		"error-stack-parser": "^2.1.4",
		"esm-env": "^1.2.2",
		"launch-editor": "^2.12.0"
	}
}
