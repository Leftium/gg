{
	"name": "@leftium/gg",
	"version": "0.0.15",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Leftium/gg.git"
	},
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && npx terser dist/debug/src/browser.js --compress --mangle -o dist/debug/src/browser.js && npx terser dist/debug/src/node.js --compress --mangle -o dist/debug/src/node.js && npx terser dist/debug/src/common.js --compress --mangle -o dist/debug/src/common.js && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*",
		"patches/"
	],
	"sideEffects": [
		"**/*.css"
	],
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.1",
		"@eslint/js": "^9.39.1",
		"@picocss/pico": "^2.1.1",
		"@sveltejs/adapter-vercel": "^6.1.1",
		"@sveltejs/kit": "^2.48.4",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/debug": "^4.1.12",
		"@types/node": "^24.10.0",
		"add": "^2.0.6",
		"debug": "^4.4.3",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.13.0",
		"globals": "^16.5.0",
		"ms": "^2.1.3",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.15",
		"svelte": "^5.43.3",
		"svelte-check": "^4.3.3",
		"terser": "^5.36.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.3",
		"vite": "^7.2.0",
		"vite-plugin-devtools-json": "^1.0.0"
	},
	"keywords": [
		"svelte"
	],
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		],
		"patchedDependencies": {
			"debug@4.4.3": "patches/debug@4.4.3.patch"
		}
	},
	"dependencies": {
		"dotenv": "^17.2.3",
		"error-stack-parser": "^2.1.4",
		"esm-env": "^1.2.2",
		"launch-editor": "^2.12.0"
	}
}
